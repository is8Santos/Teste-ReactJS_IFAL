>\n            <thead>\n                <tr>\n                    <th> list </th>\n                </tr>\n            </thead>\n\n            <tbody>\n                {newlist.map( (valueNewList, index) => (\n                    <tr key={index}>\n                    <th><input type=\"checkbox\"/> {valueNewList}</th>\n                </tr>\n                ))}\n            </tbody>\n        </table>\n        </>\n    )\n}\n\nexport default Todo;\n","/home/italo/teste_pweb/src/container/App.js",[],{"ruleId":"19","replacedBy":"20"},{"ruleId":"21","replacedBy":"22"},{"ruleId":"23","severity":1,"message":"24","line":19,"column":25,"nodeType":"25","messageId":"26","endLine":19,"endColumn":26,"fix":"27"},"no-native-reassign",["28"],"no-negated-in-lhs",["29"],"rest-spread-spacing","Unexpected whitespace after spread operator.","SpreadElement","unexpectedWhitespace",{"range":"30","text":"31"},"no-global-assign","no-unsafe-negation",[614,615],""]